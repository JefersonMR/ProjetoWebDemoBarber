<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <servlet>
        <servlet-name>CCliente</servlet-name>
        <servlet-class>br.com.Controle.CCliente</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CCliente</servlet-name>
        <url-pattern>/CCliente</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CProduto</servlet-name>
        <servlet-class>br.com.Controle.CProduto</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CProduto</servlet-name>
        <url-pattern>/CProduto</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>CServiço</servlet-name>
        <servlet-class>br.com.Controle.CServiço</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CServiço</servlet-name>
        <url-pattern>/CServiço</url-pattern>
    </servlet-mapping>
    
    
</web-app>
